{
  "plugins": [
    "syntax-object-rest-spread",
    "syntax-dynamic-import",
    ["transform-class-properties", { "loose": true }]
  ],
  "presets": [
    "react",
    [
      "env",
      {
        "modules": false,
        "loose": true,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions"
          ]
        }
      }
    ]
  ]
}
